p_marks = 50
total = 60
if (t_marks >= 35 and p_marks >= 50 and total >=60):
    print("pass")
else:
    print("fail")


#age check
age = int(input("enter age of a person:"))
if (age<=13):
    print("the person is child")
elif (age<=20):
    print("the person is teen")
else:
    print("the person is adult")

#grade selection
marks = int(input("enter the marks:"))
if (marks >=90):
    print("A grade")
elif(marks >= 70 or marks <=89):
    print("B grade")
elif(marks >= 50 or marks <= 69):
    print("C grade")
elif(marks >= 50 or marks <= 40):
    print("D grade")
else:
    print("invalid choice!")


#week of the day
num = int(input("enter a number"))
if (num == 1 ):
    print("sumday")
elif(num == 2):
    print("monday")
elif (num== 3):
    print("teusday")
elif (num == 4):
    print("wednesday")
elif( num == 5):
    print("thursday")
elif( num == 6):
    print("friday")
elif (num == 7):
    print("saturday")
else:
    print("invalid choice!")


