print("a*b:",a*b)
print("a/b:",a/b)


#logical operator
a = 10
b = 20
if(a>0 and b>10):
    print("true for AND commnd")
elif(a>0 and b<10):
    print("true for OR command")
elif(not a!=0):
    print("true for NOT command")
else:
    print("invalid choice")


#print +ve,-veand zero
n = int(input("enter a number:"))
if(n>0):
    print("the number is positive")
elif(n<0):
    print("the number is negative")
elif(n==0):
    print("the number is zero")
else:
    print("invalid choce!")



#check the number is even or odd
num = int(input("enter a number:"))
if(num%2==0):
    print("the number is even")
else:
    print("the number is odd")


#check the number is divisible bvy 5 and 11
n = int(input("enter a number:"))
if(n%5==0 and n%11==0):
    print("the number is div by 5 and 11")
else:
    print("num id not div by 5 and 11")


#ATM card 
atm_card = input(" ")
